<script type="text/javascript">
  $(document).ready(function(){ 
     $('#nameoftheindustryvisited,#learningpoints,#actionplan').on("input",function ()  {
		
		if($('#nameoftheindustryvisited').val().length <= 100)
		{
		  $('#max-name').text($('#nameoftheindustryvisited').val().length+'/100');
		}
		if($('#learningpoints').val().length <= 100)
		{
		  $('#max-learning').text($('#learningpoints').val().length+'/100');
		}
		if($('#actionplan').val().length <= 100)
		{
		  $('#max-action').text($('#actionplan').val().length+'/100');
		}
       });
 });
